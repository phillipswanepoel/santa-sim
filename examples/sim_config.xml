<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

        <replicates>1</replicates>

        <simulation>

            <genome>
                <length>10000</length>
	            <sequences>
                   
	            </sequences>	            
           </genome>

            <population>
                <populationSize>4500</populationSize>
	            <inoculum>all</inoculum>
            </population>
		
		<populationType>staticPopulation</populationType>


		<recombinationHotSpots>
			<recombinationHotSpot>				
				<feature>genome</feature>
				<sites>6000,6100</sites>
				<boostFactor>2</boostFactor>
			</recombinationHotSpot>
        </recombinationHotSpots>	


            <fitnessFunction>  
                <purifyingFitness>                                    
                    <sites>1044-1047, 1098-1101, 3300-3303, 3498-3501, 9000-9003</sites>
                    <rank>
                        <order>observed</order>
                        <breakTies>random</breakTies>
                    </rank>
                    <fitness>
                        <lowFitness>0.95</lowFitness>
                        <minimumFitness>0.2</minimumFitness>
                    </fitness>
                </purifyingFitness>   
            </fitnessFunction>

	        <mutator>
	            <nucleotideMutator>
	                <mutationRate>3.5E-5</mutationRate>
	                <rateBias>
	                    0.42 2.49 0.29
	                    1.73 0.23 4.73
	                    6.99 0.20 0.60
	                    1.02 2.56 0.88
	                </rateBias>
	            </nucleotideMutator>
	        </mutator>

	        <replicator>
	            <recombinantReplicatorWithHotspots>
			<dualInfectionProbability>0.1</dualInfectionProbability>
			<recombinationProbability>0.06</recombinationProbability>
		</recombinantReplicatorWithHotspots>
	        </replicator>

            <epoch>
                <name>epoch1</name>
                <generationCount>5000</generationCount>
            </epoch>

           
            <samplingSchedule>

                 <!-- Sample genotypes at certain times in FASTA Format -->
                <sampler>
                    <atFrequency>5000</atFrequency>
                    <fileName>alignment_0.fa</fileName>
                    <alignment>
                        <sampleSize>100</sampleSize>
                        <format>FASTA</format>
                        <label>seq_%g_%s</label>
                    </alignment>
                </sampler> 

            </samplingSchedule>
        </simulation>
</santa>
